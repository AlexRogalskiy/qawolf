{
  "name": "qawolf",
  "version": "0.0.1",
  "description": "Effortless smoke tests",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:lib": "tsc -p .",
    "build:web": "rollup --config rollup.web.ts",
    "cli": "babel-node --extensions \".ts,.tsx\" -- src/cli.ts",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build:lib && npm run build:web",
    "test": "npm run build:web && jest --maxWorkers=75%",
    "test:services": "docker-compose up -d",
    "test:watch": "npm run build:web && jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jperl/qawolf.git"
  },
  "bugs": {
    "url": "https://github.com/jperl/qawolf/issues"
  },
  "homepage": "https://github.com/jperl/qawolf#readme",
  "dependencies": {
    "clear": "^0.1.0",
    "commander": "^3.0.1",
    "dotenv": "^8.1.0",
    "fs-extra": "^8.1.0",
    "ink": "^2.3.0",
    "lodash": "^4.17.15",
    "puppeteer": "^1.20.0",
    "react": "^16.9.0",
    "rrweb": "^0.7.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/clear": "^0.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "@types/puppeteer": "^1.19.1",
    "@types/selenium-standalone": "^6.15.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "selenium-standalone": "^6.16.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "bin": {
    "qawolf": "./dist/cli.js"
  }
}
